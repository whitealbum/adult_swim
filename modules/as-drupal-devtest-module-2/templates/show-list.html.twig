{% for video in videos %}
    <div class="col-md-4">

        <h3>
            <a href="/drupal-max/videos/squidbillies/{{ video._id }}">
                {{ video.name }}
            </a>
        </h3>

        <div class="embed-responsive embed-responsive-16by9">
            <iframe class="embed-responsive-item" src="http://youtube.com/embed/{{ video.file }}" frameborder="0"></iframe>
        </div>

    </div>
{% endfor %}
